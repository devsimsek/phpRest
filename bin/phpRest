    $buf = ob_get_clean();
    ob_start();
    echo substr($buf, 0, strpos($buf, file(__FILE__)[0]));
}
$phpRestCLI = 1.1;
require_once "core/library/Cli.php"; // if we dont require our cli library application won't work!

$cli = new Cli();
$cli->parse($argv);